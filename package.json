{
  "name": "kodex",
  "displayName": "kodex",
  "description": "Dark nice theme with smooth colors",
  "version": "0.2.6",
  "publisher": "pnx",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Themes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pnx/kodex-vscode"
  },
  "contributes": {
    "themes": [
      {
        "label": "Kodex",
        "theme": "default",
        "uiTheme": "vs-dark",
        "path": "./themes/default.json"
      },
      {
        "label": "Kodex Arctic",
        "theme": "arctic",
        "uiTheme": "vs-dark",
        "path": "./themes/arctic.json"
      }
    ]
  },
  "devDependencies": {
    "@types/chroma-js": "^2.1.4",
    "@types/node": "^18.11.0",
    "chroma-js": "^2.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "build": "ts-node ./src/build.ts",
    "package": "vsce package",
    "publish": "vsce publish"
  }
}
